        android:gravity="center"
        android:textSize="@dimen/txt_encabezado"
        android:textColor="@color/txtNomTnd"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/rt_varstrinicioesc32"
        android:textSize="@dimen/txt_encabezado"
        android:textColor="@color/txtEncabezado"
        />

    <ImageView
        android:id="@+id/rt_LEimgVwLogoEncargalo"
        android:layout_width="match_parent"
