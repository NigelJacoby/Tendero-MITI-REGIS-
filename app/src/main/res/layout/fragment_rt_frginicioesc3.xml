    android:orientation="vertical"
    tools:context=".UI.Mi_tienda.rt_frginicioesc3">


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/rt_varstrinicioesc31"
        android:textSize="@dimen/txt_encabezado"
        android:textColor="@color/txtEncabezado"
        />


    <TextView
        android:id="@+id/rt_NTtxtNombreTienda"
        android:layout_width="match_parent"
